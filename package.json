{
  "name": "wp4docker",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "webpack",
    "build": "webpack -p",
    "savedb": "docker exec database sh -c 'exec mysqldump --all-databases -uroot -p\"$MYSQL_ROOT_PASSWORD\"' > ./initdb/data.sql",
    "wp": "docker run -it --rm --volumes-from wordpress --network container:wordpress wordpress:cli"
  },
  "author": "Luis Amador",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:MrLuisAmador/wp4docker.git"
  },
  "browserslist": [
    "> .5%",
    "last 2 versions",
    "Firefox ESR",
    "IE 9"
  ],
  "babel": {
    "presets": [
      "@babel/react",
      [
        "@babel/env",
        {
          "loose": true,
          "modules": false,
          "debug": true,
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": []
  },
  "engines": {
    "node": ">=8.*"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.31",
    "@babel/preset-env": "^7.0.0-beta.31",
    "@babel/preset-flow": "^7.0.0-beta.31",
    "@babel/preset-react": "^7.0.0-beta.31",
    "@babel/preset-typescript": "^7.0.0-beta.31",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "postcss-loader": "^2.0.8",
    "style-loader": "^0.19.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.31"
  }
}
