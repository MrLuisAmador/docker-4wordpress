{
  "name": "docker-webpack4wp",
  "version": "1.0.3",
  "description": "This is intended to be used as an aid to a new WordPress project",
  "scripts": {
    "webpack": "webpack",
    "init": "docker-compose up -d",
    "start": "docker-compose up -d && rimraf webpack4wp-theme/assets && npm run webpack -- --mode development --watch",
    "stop": "docker-compose stop",
    "prod": "rimraf webpack4wp-theme/assets && npm run webpack -- --mode production",
    "build": "webpack -p",
    "savedb": "docker exec database sh -c 'exec mysqldump --all-databases -uroot -p\"$MYSQL_ROOT_PASSWORD\"' > ./initdb/data.sql",
    "wp": "docker run -it --rm --volumes-from wordpress --network container:wordpress wordpress:cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrLuisAmador/docker-webpack4wp"
  },
  "keywords": [
    "webpack",
    "frontend",
    "WordPress",
    "Starter Kit"
  ],
  "author": "Luis Amador",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrLuisAmador/docker-webpack4wp/issues"
  },
  "homepage": "https://github.com/MrLuisAmador/docker-webpack4wp/blob/develop/README.md",
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@wordpress/browserslist-config": "^2.1.2",
    "ajv": "^6.1.1",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.24.5",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^1.1.5",
    "node-sass": "^4.9.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-nesting": "^4.2.1",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.1",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-wordpress": "^12.0.0",
    "stylelint-csstree-validator": "^1.2.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  }
}
